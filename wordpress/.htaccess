# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /corus-wp-exercise-202008/wordpress/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /corus-wp-exercise-202008/wordpress/index.php [L]
</IfModule>

# END WordPress
